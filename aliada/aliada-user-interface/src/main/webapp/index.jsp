  <body class="bluebackground">				
  	<div id="logon">
		<div id="fieldsetContent">
			<div id="logonWelcome"><img src="images/aliada-logo.png"/></div>
			<div id="fields">
				<div id="row">
					<div class="namePassword">User:</div>
					<div ><input id="inputUser" type="text" class="input" name="name"size="15"></input></div>
				</div>
				<div id="row">
					<div class="namePassword"><span>Password:</span></div>
					<div><input id="inputPassword" class="input" type="password" size="15"></input></div>
				</div>	
				<div id="row">
					<div id="changeLocale">
						<select name="locale">			
							 <option value="en" selected="selected">English</option>
							 <option value="es">Spanish</option>
							 <option value="ita">Italian</option>
							 <option value="hun">Hungarian</option>
					    </select>
						<input id="enter" class="button" type="submit" value="Log in"></input>
					</div>
				</div>
			</div>
			<div class="copyrightLogin">Copyright © 2014 Aliada Consortium</div>
		</div>
	</div>
</body>
