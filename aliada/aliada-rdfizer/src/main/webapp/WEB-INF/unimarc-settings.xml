<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="  
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">
       
	<bean id="work-uniform-title" class="eu.aliada.rdfizer.pipeline.format.marc.selector.FirstMatch">
		<constructor-arg>
			<list>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="500a" />
				</bean>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="501a" />
				</bean>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="200a" />
				</bean>				
			</list>
		</constructor-arg>
	</bean>

	<bean id="work-date" class="eu.aliada.rdfizer.pipeline.format.marc.selector.FirstMatch">
		<constructor-arg>
			<list>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="500k" />
				</bean>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="501k" />
				</bean>				
			</list>
		</constructor-arg>
	</bean>

	<bean id="work-other-standard-identifier" class="eu.aliada.rdfizer.pipeline.format.marc.selector.FirstMatch">
		<constructor-arg>
			<list>
				<bean
					class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="500l" />
				</bean>
				<bean
					class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="501l" />
				</bean>
			</list>
		</constructor-arg>
	</bean>
	<bean id="expression-date" class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMatches">
		<constructor-arg>
			<list/>
		</constructor-arg>
	</bean>

	<bean id="expression-language" class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMatches">
		<constructor-arg>
			<list>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="500m" />
				</bean>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="501m" />
				</bean>	
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="101a" />
				</bean>							
			</list>
		</constructor-arg>
	</bean>
	<bean id="expression-other-distinguish-feature" class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMatches">
		<constructor-arg>
			<list>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="500q" />
				</bean>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.VariableFieldExpression">
					<constructor-arg value="501q" />
				</bean>				
			</list>
		</constructor-arg>
	</bean>	
	<bean id="manifestation-control-number" class="eu.aliada.rdfizer.pipeline.format.marc.selector.FirstMatch">
		<constructor-arg>
			<list>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.ControlFieldExpression">
					<constructor-arg value="001" />
				</bean>
			</list>
		</constructor-arg>
	</bean>
	<bean id="item-identifier" class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
		<constructor-arg>
			<list>
				<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
					<constructor-arg value="852()a" />
				</bean>
			</list>
		</constructor-arg>
	</bean>
	<bean id="concept-term" class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
		<constructor-arg>
			<list/>
		</constructor-arg>
	</bean>

	<bean id="event-term"
		class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
		<constructor-arg>
			<list/>
		</constructor-arg>
	</bean>
	<bean id="place-term" class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
		<constructor-arg>
			<list />
		</constructor-arg>
	</bean>
	<bean id="workDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.WorkDetector">
		<constructor-arg ref="work-uniform-title"/>
		<constructor-arg>
	      	<list>
	      		<ref bean="work-date"/>
	      		<ref bean="work-other-standard-identifier"/>
	      	</list>
		</constructor-arg>
	</bean>
	<bean id="expressionDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.ExpressionDetector">
		<constructor-arg>
			<list>
				<ref bean="expression-date" />
				<ref bean="expression-language" />
				<ref bean="expression-other-distinguish-feature" />
			</list>
		</constructor-arg>
	</bean>
				
	<bean id="manifestationDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.ManifestationDetector">
      <constructor-arg ref="manifestation-control-number" />
	</bean>	
	
	<bean id="personDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      	    <bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
					<constructor-arg>
	      				<list>
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="700()alpha" />
							</bean>	
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="701()alpha" />
							</bean>	
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="702()alpha" />
							</bean>								
						</list>
					</constructor-arg>
				</bean>			
	      	</list>
		</constructor-arg>
		<constructor-arg value="PERSON"/>
	</bean>
	
	<bean id="familyDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      	    <bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
					<constructor-arg>
	      				<list>
			      	        <bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="720()alpha" />
							</bean>	
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="721()alpha" />
							</bean>
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="722()alpha" />
							</bean>	
						</list>
					</constructor-arg>		
				</bean>
	      	</list>
		</constructor-arg>
		<constructor-arg value="FAMILY"/>		
	</bean>
	
	<bean id="corporateBodyDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      	    <bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.AllMultipleMatches">
	      	    	<constructor-arg>
		      			<list>     	    
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="710()alpha" />
							</bean>	
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="711()alpha" />
							</bean>					
							<bean class="eu.aliada.rdfizer.pipeline.format.marc.selector.xml.MultipleVariableFieldExpression">
								<constructor-arg value="712()alpha" />
							</bean>					
						</list>
					</constructor-arg>
				</bean>		
			</list>
		</constructor-arg>
		<constructor-arg value="CORPORATE"/>		
	</bean>
	
	<bean id="itemDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      		<ref bean="item-identifier"/>
	      	</list>
		</constructor-arg>
		<constructor-arg value="ITEM"/>			
	</bean>
	
	<bean id="conceptDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      		<ref bean="concept-term"/>
	      	</list>
		</constructor-arg>
		<constructor-arg value="CONCEPT"/>		
	</bean>
	
	<bean id="eventDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      		<ref bean="event-term"/>
	      	</list>
		</constructor-arg>
		<constructor-arg value="EVENT"/>			
	</bean>
	
	<bean id="placeDetector" class="eu.aliada.rdfizer.pipeline.format.marc.frbr.MultiMapEntityDetector">
		<constructor-arg>
	      	<list>
	      		<ref bean="place-term"/>
	      	</list>
		</constructor-arg>
		<constructor-arg value="PLACE"/>		
	</bean>
</beans>